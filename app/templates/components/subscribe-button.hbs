{{#if subscribed}}
<button class="uk-button uk-button-success fa fa-check" {{action 'unsubscribeChannel' id}}>
  {{t 'components.subscribe_button.subscribed'}}
</button>
{{else}}
<button class="uk-button uk-button-primary" {{action 'subscribeChannel' id}}>
  {{t 'components.subscribe_button.subscribe'}}
</button>
{{/if}}
