<ol>
  {{#each channel in subscriptions}}
  <li id="channel-{{unbound channel.id}}" {{bind-attr class="channel.to_view:new:ok"}}>
    {{#link-to 'channel' channel.id}}
    <span class="title">{{channel.title}}</span>
    {{#if channel.to_view}}<i>{{channel.to_view}}</i>{{/if}}
    {{/link-to}}
  </li>
  {{/each}}
</ol>

